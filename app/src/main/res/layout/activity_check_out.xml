<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background2"
    tools:context=".CheckOut">


    <ImageButton
        android:id="@+id/backc"
        android:layout_width="60dp"
        android:layout_height="48dp"
        android:background="#00FFFFFF"
        android:src="@drawable/ic_vector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        tools:ignore="SpeakableTextPresentCheck"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/checkout"
        android:layout_width="116dp"
        android:layout_height="20dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/numans"
        android:text="Checkout"
        android:textColor="#2B3467"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038" />


    <TextView
        android:id="@+id/medipay"
        android:layout_width="194dp"
        android:layout_height="28dp"
        android:fontFamily="@font/nunito_sans"
        android:padding="5dp"
        android:textColor="#2B3467"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.126" />

    <TextView
        android:id="@+id/namareceiver"
        android:layout_width="274dp"
        android:layout_height="32dp"
        android:fontFamily="@font/nunito_sans"
        android:padding="5sp"
        android:textColor="#2B3467"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.234" />

    <TextView
        android:id="@+id/no_receiver"
        android:layout_width="274dp"
        android:layout_height="28dp"
        android:fontFamily="@font/nunito_sans"
        android:padding="5dp"
        android:textColor="#2B3467"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namareceiver"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/price"
        android:layout_width="170dp"
        android:layout_height="21dp"
        android:fontFamily="@font/nunito_sans"
        android:paddingRight="10sp"
        android:textAlignment="textEnd"
        android:textColor="#2B3467"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.353" />

    <TextView
        android:id="@+id/deliverypr"
        android:layout_width="170dp"
        android:layout_height="21dp"
        android:fontFamily="@font/nunito_sans"
        android:paddingRight="10sp"
        android:textAlignment="textEnd"
        android:textColor="#2B3467"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/price"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/totpay"
        android:layout_width="170dp"
        android:layout_height="21dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="72dp"
        android:fontFamily="@font/nunito_sans"
        android:paddingRight="10sp"
        android:textAlignment="textEnd"
        android:textColor="#2B3467"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deliverypr"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/delivery1"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:fontFamily="@font/nunito_sans"
        android:textColor="#2B3467"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947" />

    <Button
        android:id="@+id/btPay"
        android:layout_width="97dp"
        android:layout_height="37dp"
        android:background="@drawable/ic_kotak1"
        android:fontFamily="@font/inter_medium"
        android:text="Pay"
        android:textAllCaps="false"
        android:textColor="#FFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />



</androidx.constraintlayout.widget.ConstraintLayout>